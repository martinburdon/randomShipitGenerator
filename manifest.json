{
	"name": "Random Shipit Generator",
	"version": "0.0.14",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": ["https://github.com/*", "http://github.com/*"],
			"js": [
				"libs/jQuery.min.js",
				"content.js"
			],
			"css": [
				"css/styles.css"
			]
		}
	],
	"icons": {
		"16": "images/16x16-normal.png",
		"48": "images/48x48-normal.png",
		"128": "images/128x128-normal.png"
	},
	"web_accessible_resources": ["images/xmas/38x38.png"]
}
